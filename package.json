{
  "name": "iteris-dashboard",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/filipefcl",
  "dependencies": {
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.4.11",
    "ajv": "5.0.0",
    "axios": "^0.18.0",
    "bootstrap": "4.1.3",
    "express": "^4.17.1",
    "halogen": "^0.2.0",
    "html2canvas": "^1.0.0-rc.3",
    "jspdf": "^1.5.3",
    "moment": "2.22.2",
    "node-sass-chokidar": "1.3.3",
    "nouislider": "11.1.0",
    "npm-run-all": "4.1.3",
    "opera": "0.0.10",
    "perfect-scrollbar": "1.4.0",
    "react": "16.4.2",
    "react-big-calendar": "0.19.2",
    "react-bootstrap-sweetalert": "4.4.1",
    "react-bootstrap-switch": "15.5.3",
    "react-bootstrap-wizard": "0.0.5",
    "react-datetime": "2.15.0",
    "react-dom": "16.4.2",
    "react-icons": "^3.7.0",
    "react-input-mask": "^2.0.4",
    "react-jvectormap": "0.0.3",
    "react-loading-screen": "0.0.17",
    "react-native-indicator": "^1.0.0",
    "react-notification-alert": "0.0.8",
    "react-notification-badge": "^1.5.1",
    "react-portal-tooltip": "^2.4.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-select": "2.0.0",
    "react-spinner-material": "^1.1.3",
    "react-table": "6.8.6",
    "react-tagsinput": "3.19.0",
    "reactstrap": "6.4.0",
    "set-default-browser": "^1.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0"
  },
  "optionalDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7"
  }
}
